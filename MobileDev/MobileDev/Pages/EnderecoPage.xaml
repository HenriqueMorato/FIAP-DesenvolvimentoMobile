<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="MobileDev.EnderecoPage">
	<ContentPage.Content>
		<StackLayout Orientation="Vertical" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" Spacing="15">
			<Entry x:Name="txtCep" WidthRequest="300" HeightRequest="40" Placeholder="Digite seu Email" PlaceholderColor="Silver" TextColor="Black" BackgroundColor="White" Unfocused="Handle_Unfocused" />
			<Entry x:Name="txtRua" WidthRequest="300" HeightRequest="40" Placeholder="Digite seu Email" PlaceholderColor="Silver" TextColor="Black" BackgroundColor="White" IsEnabled="false" />
			<Entry x:Name="txtBairro" WidthRequest="300" HeightRequest="40" Placeholder="Digite seu Email" PlaceholderColor="Silver" TextColor="Black" BackgroundColor="White" IsEnabled="false"/>
			<Entry x:Name="txtCidade" WidthRequest="300" HeightRequest="40" Placeholder="Digite seu Email" PlaceholderColor="Silver" TextColor="Black" BackgroundColor="White" IsEnabled="false"/>
			<Entry x:Name="txtEstado" WidthRequest="300" HeightRequest="40" Placeholder="Digite seu Email" PlaceholderColor="Silver" TextColor="Black" BackgroundColor="White" IsEnabled="false"/>
			<Entry x:Name="txtNumero" WidthRequest="300" HeightRequest="40" Placeholder="Digite seu Email" PlaceholderColor="Silver" TextColor="Black" BackgroundColor="White" />
		</StackLayout>
	</ContentPage.Content>
</ContentPage>
